class Solution {
    public int numJewelsInStones(String js, String st) {
        int count =0;
        for(int i=0;i<js.length();i++){
            for(int j=0;j<st.length();j++){
                if(js.charAt(i)==st.charAt(j)){
                    count++;
                }
            }
        }
        return count;
    }
}
